{
  "name": "react_without-cra",
  "version": "1.0.0",
  "description": "make react app without CRA",
  "main": "index.js",
  "scripts": {
    "start": "npm build:dev && node ./dist/server.js",
    "start:wds": "webpack-dev-server --env=dev --profile --colors",
    "build": "rm -rf dist/ && NODE_ENV=production npm build:client && NODE_ENV=production npm build:server",
    "build:dev": "rm -rf dist/ && NODE_ENV=development npm build:client && NODE_ENV=development npm build:server",
    "build:server": "webpack --env=server --progress --profile --colors",
    "build:client": "webpack --env=client --progress --profile --colors",
    "test": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elli-Jeon/react_without-cra.git"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Elli-Jeon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Elli-Jeon/react_without-cra/issues"
  },
  "homepage": "https://github.com/Elli-Jeon/react_without-cra#readme",
