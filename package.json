    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
